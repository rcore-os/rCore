{
  "llvm-target": "riscv32",
  "data-layout": "e-m:e-p:32:32-i64:64-n32-S128",
  "target-endian": "little",
  "target-pointer-width": "32",
  "target-c-int-width": "32",
  "os": "none",
  "arch": "riscv32",
  "cpu": "generic-rv32",
  "features": "",
  "max-atomic-width": "32",
  "linker": "riscv64-unknown-elf-ld",
  "linker-flavor": "ld",
  "pre-link-args": {
    "ld": [
      "-Tsrc/arch/riscv32/boot/linker.ld",
      "-melf32lriscv"
    ]
  },
  "executables": true,
  "panic-strategy": "abort",
  "relocation-model": "static",
  "abi-blacklist": [
    "cdecl",
    "stdcall",
    "fastcall",
    "vectorcall",
    "thiscall",
    "aapcs",
    "win64",
    "sysv64",
    "ptx-kernel",
    "msp430-interrupt",
    "x86-interrupt"
  ]
}